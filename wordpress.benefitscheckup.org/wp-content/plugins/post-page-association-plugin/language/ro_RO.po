msgid "Order:"
msgstr "Ordoneaza:"

#: ../meta_box.php:107
msgid "Descending"
msgstr "Descrescator"

#: ../meta_box.php:108
msgid "Ascending"
msgstr "Crescator"

#: ../meta_box.php:123
msgid "Template"
msgstr "Sablon"

#: ../meta_box.php:135
