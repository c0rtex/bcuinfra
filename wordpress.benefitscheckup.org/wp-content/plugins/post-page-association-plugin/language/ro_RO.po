#: ../options_page.php:36
#: ../options_page.php:38
msgid "by"
msgstr "de"

#: ../options_page.php:1
msgid "Position"
msgstr "Pozitie"

#: ../options_page.php:4
msgid "Append the associated posts to the pages content."
msgstr "Ataseaza articolele asociate la sfarsitul paginilor."

#: ../options_page.php:8
msgid "Prepend the associated posts to the pages content."
msgstr "Ataseaza articolele asociate la inceputul paginilor."

#: ../options_page.php:12
msgid "<b>Do not</b> show the associated posts to the pages automatically. (In this case you have to add the short code manually.)"
msgstr "<b>NU</b> afisa articolele asociate automat pe pagini. (In acest caz va trebui sa adaugi scurtatura manual.)"

#: ../options_page.php:25
msgid "Templates"
msgstr "Sabloane"

#: ../options_page.php:41
#, php-format
msgid "Found in <em>%s</em>."
msgstr "Gasite in <em>%s</em>."

#: ../options_page.php:50
msgid "Do not show any text of the associated posts. (Only the title with link to the post.)"
msgstr "Nu afisa niciun text al articolelor asociate. (Doar titlul cu legatura catre articol.)"

#: ../options_page.php:55
msgid "Show an excerpt of the associated posts."
msgstr "Afiseaza un rezumat al articolelor asociate."

#: ../options_page.php:60
msgid "Show a post thumbnail of the associated posts."
msgstr "Afiseaza o pictograma a articolelor asociate."

#: ../options_page.php:65
msgid "Show the full text of the associated posts."
msgstr "Afiseaza tot textul articolelor asociate."

#: ../options_page.php:68
msgid "Miscellaneous"
msgstr "Diverse"

#: ../options_page.php:71
msgid "Please hide the Associated Posts Pro banner. Of course I will buy it."
msgstr "Te rog sa nu mai afisezi reclama la versiunea profesionala. Desigur, o voi cumpara."

