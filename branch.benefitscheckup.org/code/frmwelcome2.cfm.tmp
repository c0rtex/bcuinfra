								if (!validatezip(el)) {
									<!---alert('zip was found invalid');--->
									saveThisZip = el.value;
									el.focus();
									el.select();
								}
							}
						}
					// -->
					</script>
					<cfoutput>
					    <input class="zip_code_input" type="text" name="zip" 
					    		size="5" maxlength="5" value="#matchVal#" onBlur="javascript:zipValidJR(this);" />
					</cfoutput>					
				</p>
