#main-nav, .menu-main-menu-container
{
	height: 30px;
	width: 936px;
	margin: 0 auto;
}
#main-nav LI
{
	margin-top: 0;
	height: 30px;
	width: 155px;
	float: left;
	font-size: 20px;
	line-height: 30px;
	text-align: center;
	background:#f1601b;
        border-left:none;
	border-right:solid 1px #fff;
}

#main-nav UL
{
	list-style-type: none;
	margin: 0;
	padding: 0;
}

#main-nav UL#home LI#nav-home, #main-nav UL#find-help LI#nav-find-help, #main-nav UL#about-us LI#nav-about-us, #main-nav UL#our-sponsors LI#nav-our-sponsors, #main-nav UL#news LI#nav-news, #main-nav UL#donate LI#nav-donate
{
	background: #FDB813;
}
#main-nav UL#home LI#nav-home A, #main-nav UL#find-help LI#nav-find-help A, #main-nav UL#about-us LI#nav-about-us A, #main-nav UL#our-sponsors LI#nav-our-sponsors A, #main-nav UL#news LI#nav-news A, #main-nav UL#donate LI#nav-donate A
{
	color: #1F419B;
	text-shadow: none; !important;
}

#main-nav li#nav-home {
width:120px;
}

#main-nav li#findfood {
width:180px;
}


#main-nav li#nav-find-help{
width:180px;
}

#main-nav li#taxrelief {
width:160px;
}

#main-nav li#about {
width:134px;
border-right:none;
}

#main-nav li#benefits {
width:155px;
}



#left-menu-item
{
	border-left: 1px solid #CCCCCC !important;
}
#right-menu-item
{
	border-right: 1px solid #CCCCCC !important;
}